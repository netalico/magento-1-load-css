<?xml version="1.0"?>
<config>
    <modules>
        <Netalico_CSSPreload>
            <version>1.0.0</version>
        </Netalico_CSSPreload>
    </modules>
    <global>
        <blocks>
            <csspreload>
                <class>Netalico_CSSPreload_Block</class>
            </csspreload>
            <page>
                <rewrite>
                    <html_head>Netalico_CSSPreload_Block_Preload</html_head>
                </rewrite>
            </page>
        </blocks>
        <helpers>
            <csspreload>
                <class>Netalico_CSSPreload_Helper</class>
            </csspreload>
        </helpers>
    </global>
    <frontend>
        <layout>
            <updates>
                <csspreload>
                    <file>csspreload.xml</file>
                </csspreload>
            </updates>
        </layout>
    </frontend>
</config>